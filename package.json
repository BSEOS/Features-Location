{
    "name": "features-location",
    "displayName": "features-location",
    "description": "locating features in a source code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:features-location.helloWorld",
        "onView:featuresView"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsWelcome": [
            {
                "view": "featuresView",
                "contents": "Search for a Feature.\n[Search](command:features-location.searchFeature)"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "features-location.clearView",
                    "when": "view == featuresView",
                    "group": "navigation"
                },
                {
                    "command": "features-location.searchFeature",
                    "when": "view == featuresView",
                    "group": "navigation"
                },
                {
                    "command": "features-location.refreshEntry",
                    "when": "view == featuresView",
                    "group": "navigation"
                }
            ]
        },
        "commands": [
            {
                "command": "features-location.searchFeature",
                "title": "Search Feature",
                "icon": {
                    "light": "media/light/search.svg",
                    "dark": "media/dark/search.svg"
                }
            },
            {
                "command": "features-location.searchFeatures",
                "title": "Search Features by description file",
                "icon": {
                    "light": "media/light/search.svg",
                    "dark": "media/dark/search.svg"
                }
            },
            {
                "command": "features-location.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "features-location.explore",
                "title": "explore"
            },
            {
                "command": "fileExplorer.openFile",
                "title": "Open File"
            },
            {
                "command": "features-location.refreshEntry",
                "title": "Refresh",
                "icon": {
                    "light": "media/light/refresh.svg",
                    "dark": "media/dark/refresh.svg"
                }
            },
            {
                "command": "features-location.clearView",
                "title": "Clear"
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "featuresView",
                    "name": "Features view",
                    "icon": "media/feature-view.svg"
                },
                {
                    "id": "fileExplorer",
                    "name": "File explorer",
                    "visibility": "hidden"
                },
                {
                    "id": "nodeDependencies",
                    "name": "Node Dependencies",
                    "visibility": "hidden"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mkdirp": "^1.0.1",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/rimraf": "^3.0.0",
        "@types/vscode": "^1.56.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.19.0",
        "get-stdin": "^9.0.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "readline-sync": "^1.4.10",
        "svd-js": "^1.1.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "mkdirp": "^0.5.5"
    }
}